{
  "name": "task-aggregator-mern",
  "version": "1.0.0",
  "description": "A task list generator",
  "homepage": ".",
  "engines": {
    "node": "16.x",
    "yarn": "1.x"
  },
  "repository": "https://github.com/DamienBates/Task-Organiser-MERN.git",
  "author": "DamienBates <80431557+DamienBates@users.noreply.github.com>",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "server": "react-scripts server.ts",
    "install-client": "yarn install",
    "heroku-postbuild": "yarn run install-client && yarn run build",
    "dev": "concurrently --kill-others-on-fail \"yarn run server\" \"yarn run start\"",
